{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Runchat Docs",
  "colors": {
    "primary": "#1956d2",
    "light": "#1956d2",
    "dark": "#1956d2"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index", "quickstart", "development"]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "Nodes",
            "pages": [
              "nodes/node-reference/input",
              "nodes/node-reference/prompt",
              "nodes/node-reference/code",
              "nodes/node-reference/API",
              "nodes/node-reference/image",
              "nodes/node-reference/runchat"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": ["api-reference/introduction", "api-reference/webhooks"]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/schema",
              "api-reference/endpoint/run"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://discord.gg/2KXUBZNZ4e",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://gwyllim.substack.com",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@runchat.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://runchat.app/dashboard"
    }
  },
  "footer": {
    "socials": {
      "bluesky": "https://runchat.bsky.social/",
      "linkedin": "https://linkedin.com/company/runchat",
      "youtube": "https://www.youtube.com/@runchat.tutorials"
    }
  }
}
